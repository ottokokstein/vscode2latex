{
  "name": "vscode2latex",
  "displayName": "vscode2latex",
  "description": "vscode2latex converts selected code into a LaTeX file, preserving VS Codeâ€™s syntax highlighting and colors. Effortlessly create nicely formatted code snippets for your LaTeX documents.",
  "version": "2.0.0",
  "publisher": "ottokokstein",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ottokokstein/vscode2latex"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode2latex.convert",
        "title": "vscode2latex: Convert Code to LaTeX"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "vscode2latex.convert"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "vscode2latex",
      "properties": {
        "vscode2latex.templateFilepath": {
          "type": "string",
          "default": "",
          "description": "Path to custom .tex template file (leave empty for default)"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "check": "npm run lint && npm run check:format"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "@types/vscode": "^1.52.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.40",
    "glob": "^7.1.6",
    "mocha": "^8.3.2",
    "typescript": "^4.2.4",
    "vscode-test": "^1.5.2"
  }
}